
<table>
  <thead>
    <tr>
      <th>Type of Postharvest Structure/Facility</th>
      <th>Name</th>
      <th>Number of Units Provided</th>
      <th>Status</th>
    </tr>
  </thead>

  <tbody>
    <% barangay.barangay_postharvest_structure_and_facility_projects.each do |barangay_postharvest_structure_and_facility_project| %>
      <tr>
        <td><%= barangay_postharvest_structure_and_facility_project.postharvest_structure_and_facility_typology.name %></td>
        <td><%= barangay_postharvest_structure_and_facility_project.name %></td>
        <td><%= barangay_postharvest_structure_and_facility_project.count_of_units_provided %></td>
        <td><%= barangay_postharvest_structure_and_facility_project.status %></td>

      </tr>
    <% end %>
  </tbody>
</table>
