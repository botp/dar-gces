<h1>Listing congressional_districts</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th></th>
      <th></th>
      <th></th>
    </tr>
  </thead>

  <tbody>
    <% @congressional_districts.each do |congressional_district| %>
      <tr>
        <td><%= congressional_district.name %></td>
        <td><%= link_to 'Show', congressional_district %></td>
        <td><%= link_to 'Edit', edit_congressional_district_path(congressional_district) %></td>
        <td><%= link_to 'Destroy', congressional_district, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Congressional district', new_congressional_district_path %>
