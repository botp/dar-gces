<h1>Listing employment_statuses</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Description</th>
      <th></th>
      <th></th>
      <th></th>
    </tr>
  </thead>

  <tbody>
    <% @employment_statuses.each do |employment_status| %>
      <tr>
        <td><%= employment_status.name %></td>
        <td><%= employment_status.description %></td>
        <td><%= link_to 'Show', employment_status %></td>
        <td><%= link_to 'Edit', edit_employment_status_path(employment_status) %></td>
        <td><%= link_to 'Destroy', employment_status, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Employment status', new_employment_status_path %>
