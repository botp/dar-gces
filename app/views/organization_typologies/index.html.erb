<h1>Listing organization_typologies</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Description</th>
      <th></th>
      <th></th>
      <th></th>
    </tr>
  </thead>

  <tbody>
    <% @organization_typologies.each do |organization_typology| %>
      <tr>
        <td><%= organization_typology.name %></td>
        <td><%= organization_typology.description %></td>
        <td><%= link_to 'Show', organization_typology %></td>
        <td><%= link_to 'Edit', edit_organization_typology_path(organization_typology) %></td>
        <td><%= link_to 'Destroy', organization_typology, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

