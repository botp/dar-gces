<h1>
<%= backlink_to "Barangay Postharvest Structure and Facility Projects", "/data_entry"
%>
</h1>

<table>
  <thead>
    <tr>
      <th>Barangay</th>
      <th>Type of Postharvest Structure/Facility</th>
      <th>Tag</th>
      <th>Name</th>
      <th>Description</th>
      <th>Number of Units Provided</th>
      <th>Status</th>
      <th>Date confirmed</th>
      <th>User</th>
<!--       <th></th>
      <th></th>
      <th></th> -->
    </tr>
  </thead>

  <tbody>
    <% @barangay_postharvest_structure_and_facility_projects.each do |barangay_postharvest_structure_and_facility_project| %>
      <tr>
        <td><%= barangay_postharvest_structure_and_facility_project.barangay.name %></td>
        <td><%= barangay_postharvest_structure_and_facility_project.postharvest_structure_and_facility_typology.name %></td>
        <td><%= barangay_postharvest_structure_and_facility_project.tag %></td>
        <td><%= barangay_postharvest_structure_and_facility_project.name %></td>
        <td><%= barangay_postharvest_structure_and_facility_project.description %></td>
        <td><%= barangay_postharvest_structure_and_facility_project.count_of_units_provided %></td>
        <td><%= barangay_postharvest_structure_and_facility_project.status %></td>
        <td><%= barangay_postharvest_structure_and_facility_project.date_confirmed %></td>
        <td><%= barangay_postharvest_structure_and_facility_project.user_id %></td>
        <td><%= link_to 'Show', barangay_postharvest_structure_and_facility_project %></td>
        <td><%= link_to 'Edit', edit_barangay_postharvest_structure_and_facility_project_path(barangay_postharvest_structure_and_facility_project) %></td>
        <td><%= link_to 'Destroy', barangay_postharvest_structure_and_facility_project, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= buttonlink_to 'New Project', new_barangay_postharvest_structure_and_facility_project_path %>
